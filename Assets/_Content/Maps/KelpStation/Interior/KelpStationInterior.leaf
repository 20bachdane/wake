# basePath interior.kelpStation

#include "Headers/Common.leaf"

:: lockExit
@function   SceneStart
@when       !JobStartedOrComplete("kelp-welcome")
$call   ExitStationGroup->LockGroup()

:: unlockExit
@function   JobStarted
@when       jobId == "kelp-welcome"
$call   ExitStationGroup->UnlockGroup()